package string;

import java.util.Iterator;

public class S1 {
	public static void main(String[] args) {
		
		//i/p:hi how are you
		//o/p:uyo era woh ih
		
		
		String s="hi how are you";
		
		System.out.println(reverse(s));
		
		
	}
	
	public static String reverse(String str) {
		
		String rev=" ";  
		for(int i=str.length()-1;i>=0;i--) {
			rev+=str.charAt(i);
		}
		return rev;
		
		
	}
}
