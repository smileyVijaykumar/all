package pattern;

public class P6 {
	// A B C D E
	// 1 2 3 4 5
	// E D C B A
	// 6 7 8 9 10
	// A B C D E
	
	public static void main(String[] args) {
		int num=5;
		
		int n=1;
		
		for(int i=0;i<num;i++) {
			char ch='A';
			char ch1='E';
			for(int j=0;j<num;j++) {
				if(i%2==1) {
					System.out.print(n++ + " ");
				}else {
					System.out.print(ch++ +" ");
				}
			}
			
			System.out.println();
		}
		
	}
}
