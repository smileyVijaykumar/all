package string;


public class S1 {
	public static void main(String[] args) {
		
		//i/p:hi how are you
		//o/p:uyo era woh ih
		
		String s="hi how are you";
		
		System.out.println(rev(s));
		
		
	}
	
	public static String rev(String s) {
		
		String rev=" ";
		
		for(int i=s.length();i>=0;i--) {
			rev+=s.charAt(i);
		}
		return rev;
	}
}
