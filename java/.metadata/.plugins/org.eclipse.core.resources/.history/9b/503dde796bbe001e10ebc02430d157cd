package string;

public class S3 {

	public static void main(String[] args) {
		
		String str = "Hello";
		int count=1;
		char [] ch= str.toCharArray();
		
		for(int i=0;i<ch.length;i++) {
			for(int j=i+1;j<ch.length-1;j++) {
				if(ch[i]==ch[j]) {
					count++;
				}
			}
		}
		System.out.println(count);
	}
	
}
